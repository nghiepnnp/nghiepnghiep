<%if(message.length > 0){%>
    <div class="alert m-0 py-2 px-3 alert-default-<%-message[0]%>" role="alert">
        <%if(message[0] == "danger"){%>
            <i class="fas fa-skull-crossbones fa-xl pr-2"></i>
            <%-message[1]%>
        <%}else if(message[0] == "success"){%>
            <i class="fas fa-check fa-xl pr-2"></i>
            <%-message[1]%>
        <%}else if(message[0] == "warning"){%>
            <i class="fas fa-exclamation-triangle fa-xl pr-2"></i>
            <%-message[1]%>
        <%}%>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
<%}%>

